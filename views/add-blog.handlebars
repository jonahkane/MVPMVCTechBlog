<article class="new-blog">
  <section>
    <div>
      <h2>Create New Blog</h2>
    </div>
    <div>
      <form class="new-blog-entry">
        <div>
          <label for="blog-title">Title</label>
          <input type="text" id="blog-title" name="blog-title" />
        </div>
        <div>
          <label for="blog-content">Content</label>
          <textarea
            class="entry-field"
            id="blog-content"
            name="blog-content"
          ></textarea>
        </div>
        <button type="submit" class="btn">Create</button>
      </form>
    </div>
  </section>
</article>
<script src="/js/add-blog.js"></script>